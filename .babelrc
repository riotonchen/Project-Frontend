{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": [
          "> 0.5%", "last 5 versions",
          "not ie <= 8",
          "last 5 iOS versions",
          "last 5 ChromeAndroid versions",
          "last 5 Android versions",
          "not dead"
        ]
      }
    }],
    "stage-2"
  ],
  "plugins": ["transform-vue-jsx", "transform-runtime"],
  "env": {
    "development": {
      "plugins": ["dynamic-import-node"]
    }
  }
}
