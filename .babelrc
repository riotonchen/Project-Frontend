{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": ["last 10 versions", "not dead"],
          "node": "current",
          "uglify": true
        }
      }
    ],
    "stage-2"
  ],
  "plugins": ["transform-vue-jsx", "transform-runtime"],
  "env": {
    "development": {
      "plugins": ["dynamic-import-node"]
    }
  }
}
